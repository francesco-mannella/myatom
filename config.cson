"*":
  Hydrogen:
    startDir: "dirOfFile"
    wrapOutput: true
  "atom-ide-terminal":
    ansiColors:
      normal:
        blue: "#4f4fed"
        red: "#f56060"
      zBright:
        brightBlue: "#5d5dee"
        brightRed: "#fc5f5f"
    core:
      workingDirectory: "Active File"
    style:
      theme: "inverse"
  "atom-latex":
    clean_after_build: true
    hide_log_if_success: true
    latexmk_param: "-synctex=1 -interaction=nonstopmode -file-line-error -pdf -bibtex -f"
    preview_after_build: "View in PDF viewer tab"
    save_on_build: true
    toolchain: "latexmk toolchain"
  "atom-package-deps":
    ignored: [
      "linter"
    ]
  "atom-python-run":
    consoleLog: false
  autocomplete:
    includeCompletionsFromAllBuffers: true
  "autocomplete-clang": {}
  "autocomplete-latex-cite":
    includeGlobalBibFiles: true
  "autocomplete-python":
    useSnippets: "required"
  autosave:
    enabled: true
  "autosave-plus":
    enabled: true
  core:
    disabledPackages: [
      "linter-ui-default"
      "markdown-preview"
    ]
    packagesWithKeymapsDisabled: []
    telemetryConsent: "no"
    themes: [
      "rzx-light-ui"
      "contrast-light-syntax"
    ]
    useTreeSitterParsers: false
  "docblock-python":
    style: "google"
    types: {}
    use_defaults: true
  editor:
    fontSize: 16
    tabType: "soft"
  "ex-mode":
    gdefault: true
  "exception-reporting":
    userId: "240f309c-fea9-47c5-86d6-29b068d3732e"
  latex:
    opener: "evince"
    outputDirectory: "pdf"
  "linter-pylama":
    ignoreErrorsAndWarnings: "D203,D212,D213,D404,D100,W0611,D214,D215,D405,D406,D407,D408,D409,D410,D411,D415,D416,D417"
    usePep257: false
  "linter-python":
    withPep8: true
  "linter-spell": {}
  "linter-ui-default":
    panelHeight: 350
  "pdf-view":
    fitToWidthOnOpen: true
    paneToUseInSynctex: "down"
  "python-autopep8":
    formatOnSave: true
  "spell-check":
    addKnownWords: true
    excludedScopes: [
      "support.function.tex"
      "meta.preamble.latex"
      "support.type.function.latex"
      "comment.line.percentage.tex"
      "storage.type.function.latex"
      "support.function.latex"
      "string.other.math.tex"
      "string.other.math.block.environment.latex"
      "variable.parameter.function.latex"
      "constant.other.reference.latex"
    ]
    grammars: [
      "source.asciidoc"
      "source.gfm"
      "text.git-commit"
      "text.plain"
      "text.plain.null-grammar"
      "source.rst"
      "text.restructuredtext"
      "text.tex.latex"
    ]
    knownWords: [
      "discretized"
      "SOM"
      "SOMs"
      "STMs"
      "variational"
      "autoencoders"
      "centroids"
      "centroid"
      "dataset"
      "iteratively"
    ]
  "symbols-navigator":
    clickType: "Single Click"
    collapsedByDefault: true
    position: "Right"
    showOnStatusBar: true
    sortBy: "Source Row"
  welcome:
    showOnStartup: false
".bibtex.text":
  editor:
    softWrap: true
